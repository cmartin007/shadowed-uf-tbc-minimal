# Copy to .env and fill in. Do not commit .env.
# Create a client at: https://develop.battle.net/access/clients

BLIZZARD_CLIENT_ID=your_client_id
BLIZZARD_CLIENT_SECRET=your_client_secret

# Region for OAuth and API (us, eu, kr, tw)
BLIZZARD_REGION=us

# Optional: locale for game data (e.g. en_US)
BLIZZARD_LOCALE=en_US
